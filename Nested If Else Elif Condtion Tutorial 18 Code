s=int(input("enter a number and hit enter"))
k=int(input("enter a number and hit enter"))
if s==5:
    if k==1:
        print("this is five and one") #nested ifelse condition
    elif k==2:
        print("this is five and two") #nested elif condition
elif s==6:
    print("six")
elif s==7:
    print("seven")
else:
    print("don't know")

print("hii")
#to give any comment we use # sign in python before the comment line
